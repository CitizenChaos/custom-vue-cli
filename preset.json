{
  "useTaobaoRegistry": true,
  "packageManager": "npm",
  "useConfigFiles": true,
  "router": true,
  "vuex": true,
  "cssPreprocessor": "less",
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-router": {
      "historyMode": false
    },
    "@vue/cli-plugin-vuex": {},
    "@vue/cli-plugin-eslint": {
      "config": "standard",
      "lintOn": ["save"]
    }
  },
  "configs": {
    "eslintConfig": {
      "rules": {
        "no-alert": 0
      }
    }
  }
}
